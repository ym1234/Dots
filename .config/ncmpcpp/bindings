def_key "j"
  scroll_down

def_key "k"
  scroll_up

def_key "h"
  jump_to_parent_directory

def_key "l"
  enter_directory

def_key "l"
  show_lyrics

def_key "G"
  move_end

def_key "g"
  move_home

def_key ";"
  jump_to_position_in_song

def_key "'"
  jump_to_browser

def_key "+"
  show_clock

def_key "="
  volume_up

def_key "d"
  delete_playlist_items

def_key "d"
  delete_browser_items

def_key "d"
  delete_stored_playlist

def_key "left"
  seek_backward

def_key "right"
  seek_forward

